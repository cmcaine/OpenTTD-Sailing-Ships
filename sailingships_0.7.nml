item (FEAT_SHIPS , item_merch) {
	property {
		name : string(STR_MERCH_NAME);
		climates_available : ALL_CLIMATES & ~bitmask(CLIMATE_TOYLAND);
		introduction_date : (param_introduction_date==0) ? date(1750,07,01) : date(1750,07,01);
